{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ArtifactsObjects",
      "Effect": "Allow",
      "Action": ["s3:PutObject","s3:GetObject","s3:DeleteObject"],
      "Resource": "arn:aws:s3:::sam-artifacts-giodegeronimo/*"
    },
    {
      "Sid": "ArtifactsListAndLocation",
      "Effect": "Allow",
      "Action": ["s3:ListBucket","s3:GetBucketLocation"],
      "Resource": "arn:aws:s3:::sam-artifacts-giodegeronimo"
    }
  ]
}
